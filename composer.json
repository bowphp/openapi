{
    "name": "bowphp/openai",
    "description": "OpenAI integration for BowPHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "gnakale",
            "role": "Bowphp team Member"
        }
    ],
    "require": {
        "php": ">=8.1",
        "openai-php/client": "^0.5.2",
        "symfony/http-client": "^6.3",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Bow\\OpenAI\\": "src/"     
           }
    },
    "require-dev": {
        "bowphp/framework": "^5.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
